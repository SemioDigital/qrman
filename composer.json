{
    "name": "semiodigital/qrman",
    "type": "project",
    "description": "Dynamic QR code, logo QR code generate, tracking QR code scan, hitmap dashboard and admin panel using Laravel 7 framework, Vuexy template and python.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "endroid/qr-code": "^3.9",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "spatie/laravel-permission": "^3.18",
        "laravel/legacy-factories": "^1.1",
        "predis/predis": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5.2",
        "beyondcode/laravel-dump-server": "^1.6.0",
        "filp/whoops": "^2.8",
        "fzaninotto/faker": "^1.9.12",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.1",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "Database\\Factories\\": "database/factories/",
        "Database\\Seeders\\": "database/seeders/",
        "classmap": [
            "database/seeders",
            "database/factories"
        ],
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
